<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.text.Font?>

<BorderPane prefHeight="760.0" prefWidth="1280.0" style="-fx-background-color: white;" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="UserInterface.JoinClassController">
   <center>
      <AnchorPane prefHeight="742.0" prefWidth="1280.0" style="-fx-background-color: white;">
         <children>
            <Label layoutX="506.0" layoutY="66.0" prefHeight="44.0" prefWidth="284.0" text="Join New Class">
               <font>
                  <Font name="Arial Black" size="27.0" />
               </font>
            </Label>
            <Label layoutX="381.0" layoutY="277.0" prefHeight="44.0" prefWidth="284.0" text="Class Code:">
               <font>
                  <Font name="Arial Black" size="18.0" />
               </font>
            </Label>
            <TextField fx:id="class_code" layoutX="581.0" layoutY="281.0" prefHeight="31.0" prefWidth="300.0" promptText="Enter Class Code">
               <font>
                  <Font name="Arial" size="17.0" />
               </font>
            </TextField>
            <Button fx:id="go_back" layoutX="149.0" layoutY="75.0" mnemonicParsing="false" onMouseClicked="#handle_go_back" style="-fx-background-color: green;" text="Back" textFill="WHITE">
               <font>
                  <Font name="Arial Bold" size="16.0" />
               </font>
            </Button>
            <Button fx:id="join_class_btn" layoutX="601.0" layoutY="479.0" mnemonicParsing="false" onMouseClicked="#handle_join_class_btn" style="-fx-background-color: green;" text="Join Class" textFill="WHITE">
               <font>
                  <Font name="Arial Bold" size="16.0" />
               </font>
            </Button>
         </children>
      </AnchorPane>
   </center>
</BorderPane>
